{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "zivotopis-batinic": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/zivotopis-batinic",
            "index": "src/index.html",
            "main": "src/main.ts",
            "allowedCommonJsDependencies": [
              "core-js",
              "raf",
              "rgbcolor",
              "html2canvas",
              "jspdf",
              "file-saver"
            ],
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              {
                "glob": "**/*",
                "input": "src/assets/fonts",
                "output": "/assets/fonts"
              }
            ],
            "styles": [
              "src/styles.css",
              "node_modules/@fontsource/roboto/400.css",
              "node_modules/@fontsource/roboto/500.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1.5MB",
                  "maximumError": "2MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "8kb",
                  "maximumError": "10kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "zivotopis-batinic:build:production"
            },
            "development": {
              "buildTarget": "zivotopis-batinic:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "/assets"
              }
            ],
            "styles": [
              "src/styles.css",
              "node_modules/@fontsource/roboto/400.css",
              "node_modules/@fontsource/roboto/500.css"
            ],
            "scripts": []
          }
        },
        "deploy": {
          "builder": "angular-cli-ghpages:deploy",
          "options": {
            "baseHref": "/zivotopis-batinic/",
            "buildTarget": "zivotopis-batinic:build:production"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "353f6971-6fc5-4458-887f-339cb0aca9b8"
  }
}
